{
  "$schema": "https://vega.github.io/schema/vega/v5.8.json",
  "width": 600,
  "height": 300,
  "padding": 0,
  "signals": [
    {
      "name": "matchedNodes",
      "update": "data('pairing').length"
    },
    {
      "name": "greenEdges",
      "update": "data('green-edges-flatten-time').length"
    },
    {
      "name": "grayEdges",
      "update": "data('candidate-edges-flatten-time').length"
    },
    {
      "name": "redEdges",
      "update": "data('template-edges-flatten-time').length - greenEdges"
    },
    {
      "name": "totalNodes",
      "value": 51
    },
    {
      "name": "timelineHeight",
      "value": 30
    },
    {
      "name": "barChartWidth",
      "value": 60
    },
    {
      "name": "trackWidth",
      "value": 600
    },
    {
      "name": "nodeSize",
      "value": 10
    },
    {
      "name": "arcWidth",
      "value": 2
    },
    {
      "name": "templateAxisYOffset",
      "value": 60
    },
    {
      "name": "candidateAxisYOffset",
      "value": 110
    },
    {
      "name": "timelineYOffset",
      "value": 160
    }
  ],
  "data": [
    {
      "name": "sortingMetrics",
      "values": [
        {
          "value": "Financial profile"
        },
        {
          "value": "Travel profile"
        },
        {
          "value": "Graphlet profile"
        },
        {
          "value": "Temporal profile"
        }
      ],
      "transform": [
        {
          "type": "window",
          "as": [
            "key"
          ],
          "ops": [
            "rank"
          ]
        }
      ]
    },
    {
      "name": "pairing",
      "url": {
        "signal": "'data/' + candidateGraphFolder + '/pairing.csv'"
      },
      "format": {
        "type": "csv"
      },
      "transform": [
        {
          "type": "formula",
          "expr": "parseFloat(datum[nodeOrderMode])",
          "as": "score"
        },
        {
          "type": "collect",
          "sort": {
            "field": "score",
            "order": "descending"
          }
        },
        {
          "type": "window",
          "ops": [
            "row_number"
          ],
          "as": [
            "order"
          ]
        }
      ]
    },
    {
      "name": "template-nodes",
      "url": {
        "signal": "'data/' + candidateGraphFolder + '/template_nodes.csv'"
      },
      "format": {
        "type": "csv"
      },
      "transform": [
        {
          "type": "filter",
          "expr": "datum.NodeType == 1"
        },
        {
          "type": "lookup",
          "from": "pairing",
          "key": "templateID",
          "fields": [
            "NodeID"
          ],
          "as": [
            "pairingObj"
          ]
        },
        {
          "type": "filter",
          "expr": "isValid(datum.pairingObj)"
        },
        {
          "type": "formula",
          "expr": "datum.pairingObj.order",
          "as": "order"
        }
      ]
    },
    {
      "name": "candidate-nodes",
      "url": {
        "signal": "'data/' + candidateGraphFolder + '/candidate_nodes.csv'"
      },
      "format": {
        "type": "csv"
      },
      "transform": [
        {
          "type": "filter",
          "expr": "datum.NodeType == 1"
        },
        {
          "type": "lookup",
          "from": "pairing",
          "key": "candidateID",
          "fields": [
            "NodeID"
          ],
          "as": [
            "pairingObj"
          ]
        },
        {
          "type": "filter",
          "expr": "isValid(datum.pairingObj)"
        },
        {
          "type": "lookup",
          "from": "template-nodes",
          "key": "NodeID",
          "fields": [
            "pairingObj.templateID"
          ],
          "as": [
            "pairedNode"
          ]
        },
        {
          "type": "formula",
          "expr": "datum.pairedNode.order",
          "as": "order"
        }
      ]
    },
    {
      "name": "template-edges",
      "url": {
        "signal": "'data/' + candidateGraphFolder + '/template_edges.csv'"
      },
      "format": {
        "type": "csv"
      },
      "transform": [
        {
          "type": "filter",
          "expr": "datum.eType == 0 || datum.eType == 1"
        },
        {
          "type": "formula",
          "as": "Status",
          "expr": "datum['Status']"
        },
        {
          "type": "formula",
          "expr": "parseInt(datum.Time)",
          "as": "Time"
        },
        {
          "type": "filter",
          "expr": "datum.Status == 'common' || datum.Status == 'missing' "
        },
        {
          "type": "lookup",
          "from": "template-nodes",
          "key": "NodeID",
          "fields": [
            "Source",
            "Target"
          ],
          "as": [
            "source",
            "target"
          ]
        },
        {
          "type": "filter",
          "expr": "isValid(datum.source) && isValid(datum.target)"
        }
      ]
    },
    {
      "name": "candidate-edges",
      "url": {
        "signal": "'data/' + candidateGraphFolder + '/candidate_edges.csv'"
      },
      "format": {
        "type": "csv"
      },
      "transform": [
        {
          "type": "filter",
          "expr": "datum.eType == 0 || datum.eType == 1"
        },
        {
          "type": "formula",
          "as": "Status",
          "expr": "datum['Status']"
        },
        {
          "type": "filter",
          "expr": "datum.Status == 'supplementary'"
        },
        {
          "type": "formula",
          "expr": "parseInt(datum.Time)",
          "as": "Time"
        },
        {
          "type": "lookup",
          "from": "candidate-nodes",
          "key": "NodeID",
          "fields": [
            "Source",
            "Target"
          ],
          "as": [
            "source",
            "target"
          ]
        },
        {
          "type": "filter",
          "expr": "isValid(datum.source) && isValid(datum.target)"
        }
      ]
    },
    {
      "name": "template-edges-flatten-time",
      "source": "template-edges",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "Source",
            "Target",
            "eType",
            "Status"
          ],
          "fields": [
            "Weight",
            null,
            "Time",
            "Time"
          ],
          "ops": [
            "sum",
            "count",
            "min",
            "max"
          ],
          "as": [
            "Weight",
            "count",
            "minTime",
            "maxTime"
          ]
        }
      ]
    },
    {
      "name": "candidate-edges-flatten-time",
      "source": "candidate-edges",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "Source",
            "Target",
            "eType",
            "Status"
          ],
          "fields": [
            "Weight",
            null,
            "Time",
            "Time"
          ],
          "ops": [
            "sum",
            "count",
            "min",
            "max"
          ],
          "as": [
            "Weight",
            "count",
            "minTime",
            "maxTime"
          ]
        }
      ]
    },
    {
      "name": "template-edges-flatten-time-and-type",
      "source": "template-edges-flatten-time",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "Source",
            "Target",
            "Status"
          ],
          "fields": [
            "Weight",
            "count",
            "eType",
            "eType"
          ],
          "ops": [
            "sum",
            "sum",
            "min",
            "max"
          ],
          "as": [
            "Weight",
            "count",
            "eType_min",
            "eType_max"
          ]
        },
        {
          "type": "formula",
          "expr": "datum.eType_min != datum.eType_max ? [datum.eType_min, datum.eType_max] : [datum.eType_min]",
          "as": "eType"
        },
        {
          "type": "lookup",
          "from": "template-nodes",
          "key": "NodeID",
          "fields": [
            "Source",
            "Target"
          ],
          "as": [
            "source",
            "target"
          ]
        }
      ]
    },
    {
      "name": "candidate-edges-flatten-time-and-type",
      "source": "candidate-edges-flatten-time",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [
            "Source",
            "Target",
            "Status"
          ],
          "fields": [
            "Weight",
            "count",
            "eType",
            "eType"
          ],
          "ops": [
            "sum",
            "sum",
            "min",
            "max"
          ],
          "as": [
            "Weight",
            "count",
            "eType_min",
            "eType_max"
          ]
        },
        {
          "type": "formula",
          "expr": "datum.eType_min != datum.eType_max ? [datum.eType_min, datum.eType_max] : [datum.eType_min]",
          "as": "eType"
        },
        {
          "type": "lookup",
          "from": "candidate-nodes",
          "key": "NodeID",
          "fields": [
            "Source",
            "Target"
          ],
          "as": [
            "source",
            "target"
          ]
        }
      ]
    },
    {
      "name": "green-edges-flatten-time",
      "source": "template-edges-flatten-time",
      "transform": [
        {
          "type": "filter",
          "expr": "datum.Status == 'common'"
        }
      ]
    }
  ],
  "marks": [
    {
      "name": "arcview",
      "type": "group",
      "data": [
        {
          "name": "track-matrix",
          "source": "pairing",
          "transform": [
            {
              "type": "fold",
              "fields": [
                "Financial profile",
                "Travel profile",
                "Graphlet profile",
                "Temporal profile"
              ],
              "as": [
                "metric",
                "value"
              ]
            },
            {
              "type": "lookup",
              "from": "candidate-nodes",
              "key": "NodeID",
              "fields": [
                "candidateID"
              ],
              "values": [
                "order"
              ]
            }
          ]
        }
      ],
      "scales": [
        {
          "name": "node-position",
          "type": "point",
          "domain": {
            "data": "template-nodes",
            "field": "order",
            "sort": true
          },
          "range": {
            "step": {
              "signal": "trackWidth/totalNodes"
            }
          }
        }
      ],
      "encode": {
        "enter": {
          "y": {
            "signal": "templateAxisYOffset"
          },
          "x": {
            "value": 0
          },
          "height": {
            "signal": "candidateAxisYOffset-templateAxisYOffset"
          },
          "width": {
            "signal": "trackWidth"
          },
          "stroke": {
            "value": "white"
          }
        }
      },
      "axes": [
        {
          "orient": "left",
          "scale": "metricScale",
          "labelBaseline": "bottom",
          "ticks": false,
          "offset": {
            "signal": "nodeSize"
          },
          "domain": false
        }
      ],
      "marks": [
        {
          "type": "symbol",
          "name": "top-nodes",
          "from": {
            "data": "template-nodes"
          },
          "encode": {
            "enter": {
              "cursor": {
                "value": "pointer"
              },
              "opacity": {
                "value": 1
              },
              "tooltip": {
                "signal": "{'Graph':'Template', 'Id': datum.NodeID, 'Paired to': datum.pairingObj.candidateID, 'Country': datum.Country, 'Order from': datum.pairingObj[nodeOrderMode] }"
              }
            },
            "update": {
              "xc": {
                "scale": "node-position",
                "field": "order"
              },
              "yc": {
                "value": 0
              },
              "size": {
                "signal": "nodeSize*nodeSize"
              },
              "fill": {
                "signal": "scale('nodeCountries', datum['Country']) || 'white'"
              },
              "stroke": {
                "value": "black"
              },
              "opacity": {
                "value": 1
              }
            },
            "hover": {
              "size": {
                "signal": "2*nodeSize*nodeSize"
              }
            }
          }
        },
        {
          "type": "symbol",
          "name": "bottom-nodes",
          "from": {
            "data": "candidate-nodes"
          },
          "encode": {
            "enter": {
              "cursor": {
                "value": "pointer"
              },
              "opacity": {
                "value": 1
              },
              "tooltip": {
                "signal": "{'Graph':'Template', 'Id': datum.NodeID, 'Paired to': datum.pairingObj.candidateID, 'Country': datum.Country, 'Order from': datum.pairingObj[nodeOrderMode] }"
              }
            },
            "update": {
              "xc": {
                "scale": "node-position",
                "field": "order"
              },
              "yc": {
                "signal": "candidateAxisYOffset-templateAxisYOffset"
              },
              "size": {
                "signal": "nodeSize*nodeSize"
              },
              "fill": {
                "signal": "scale('nodeCountries', datum['Country']) || 'white'"
              },
              "stroke": {
                "value": "black"
              },
              "opacity": {
                "value": 1
              }
            },
            "hover": {
              "size": {
                "signal": "2*nodeSize*nodeSize"
              }
            }
          }
        },
        {
          "name": "template-links",
          "type": "path",
          "from": {
            "data": "template-edges-flatten-time-and-type"
          },
          "encode": {
            "enter": {
              "cursor": {
                "value": "pointer"
              },
              "tooltip": {
                "signal": "{'Status': datum.Status, 'Source': datum.Source,'Target':datum.Target, 'Type': datum.eType }"
              }
            },
            "update": {
              "stroke": {
                "field": "Status",
                "scale": "edgeStatusColor"
              },
              "strokeOpacity": {
                "value": 0.5
              },
              "strokeWidth": {
                "signal": "arcWidth* datum.eType.length"
              },
              "opacity": {
                "value": 0.5
              }
            },
            "hover": {
              "strokeOpacity": {
                "value": 1
              }
            }
          },
          "transform": [
            {
              "type": "lookup",
              "from": "top-nodes",
              "key": "datum.NodeID",
              "fields": [
                "datum.Source",
                "datum.Target"
              ],
              "as": [
                "sourceNode",
                "targetNode"
              ]
            },
            {
              "type": "linkpath",
              "sourceX": {
                "expr": "min(datum.sourceNode.x, datum.targetNode.x)"
              },
              "targetX": {
                "expr": "max(datum.sourceNode.x, datum.targetNode.x)"
              },
              "sourceY": {
                "expr": "-nodeSize"
              },
              "targetY": {
                "expr": "-nodeSize"
              },
              "shape": "curve"
            }
          ]
        },
        {
          "name": "candidate-links",
          "type": "path",
          "from": {
            "data": "candidate-edges-flatten-time-and-type"
          },
          "encode": {
            "enter": {
              "cursor": {
                "value": "pointer"
              },
              "tooltip": {
                "signal": "{'Status': datum.Status, 'Source': datum.Source, 'Target':datum.Target, 'Type': datum.eType }"
              }
            },
            "update": {
              "stroke": {
                "field": "Status",
                "scale": "edgeStatusColor"
              },
              "strokeOpacity": {
                "value": 0.5
              },
              "strokeWidth": {
                "signal": "arcWidth*datum.eType.length"
              },
              "opacity": {
                "value": 0.5
              }
            },
            "hover": {
              "strokeOpacity": {
                "value": 1
              }
            }
          },
          "transform": [
            {
              "type": "lookup",
              "from": "bottom-nodes",
              "key": "datum.NodeID",
              "fields": [
                "datum.Source",
                "datum.Target"
              ],
              "as": [
                "sourceNode",
                "targetNode"
              ]
            },
            {
              "type": "linkpath",
              "sourceX": {
                "expr": "max(datum.sourceNode.x, datum.targetNode.x)"
              },
              "targetX": {
                "expr": "min(datum.sourceNode.x, datum.targetNode.x)"
              },
              "sourceY": {
                "expr": "candidateAxisYOffset-templateAxisYOffset+nodeSize"
              },
              "targetY": {
                "expr": "candidateAxisYOffset-templateAxisYOffset+nodeSize"
              },
              "shape": "curve"
            }
          ]
        },
        {
          "name": "track-cells",
          "type": "symbol",
          "from": {
            "data": "track-matrix"
          },
          "encode": {
            "enter": {
              "cursor": {
                "value": "pointer"
              },
              "opacity": {
                "value": 1
              },
              "tooltip": {
                "signal": "{'value':datum.value, 'similarity':datum.metric, 'templateID':datum.templateID, 'candidateID':datum.candidateID }"
              }
            },
            "update": {
              "shape": [
                {
                  "test": "isValid(toNumber(datum.value))",
                  "value": "square"
                },
                {
                  "value": "cross"
                }
              ],
              "angle": [
                {
                  "test": "isValid(toNumber(datum.value))",
                  "value": 0
                },
                {
                  "value": 45
                }
              ],
              "fill": [
                {
                  "test": "isValid(toNumber(datum.value))",
                  "field": "value",
                  "scale": "similarityScore"
                },
                {
                  "value": "lightgray"
                }
              ],
              "xc": {
                "field": "order",
                "scale": "node-position"
              },
              "yc": {
                "field": "metric",
                "scale": "metricScale"
              },
              "size": {
                "signal": "nodeSize*nodeSize"
              },
              "strokeWidth": {
                "value": 0
              },
              "opacity": {
                "value": 1
              }
            },
            "hover": {
              "stroke": {
                "value": "black"
              },
              "strokeWidth": {
                "value": 1
              }
            }
          }
        }
      ]
    },
    {
      "name": "similarity-bar-chart",
      "type": "group",
      "data": [
        {
          "name": "aggregate-metric",
          "source": "pairing",
          "transform": [
            {
              "type": "aggregate",
              "fields": [
                "Financial profile",
                "Travel profile",
                "Graphlet profile",
                "Temporal profile"
              ],
              "ops": [
                "mean",
                "mean",
                "mean",
                "mean"
              ],
              "as": [
                "Financial profile",
                "Travel profile",
                "Graphlet profile",
                "Temporal profile"
              ]
            },
            {
              "type": "fold",
              "fields": [
                "Financial profile",
                "Travel profile",
                "Graphlet profile",
                "Temporal profile"
              ],
              "as": [
                "category",
                "amount"
              ]
            },
            {
              "type": "formula",
              "as": "amount",
              "expr": "isDefined(datum.amount) ? datum.amount : 0"
            }
          ]
        }
      ],
      "signals": [
        {
          "name": "tooltip",
          "value": {},
          "on": [
            {
              "events": "rect:mouseover",
              "update": "datum"
            },
            {
              "events": "rect:mouseout",
              "update": "{}"
            }
          ]
        }
      ],
      "scales": [
        {
          "name": "xscale",
          "domain": [
            0,
            1
          ],
          "nice": true,
          "range": [
            0,
            {
              "signal": "barChartWidth"
            }
          ]
        }
      ],
      "encode": {
        "enter": {
          "y": {
            "signal": "templateAxisYOffset"
          },
          "x": {
            "signal": "matchedNodes*(trackWidth/totalNodes)"
          },
          "height": {
            "signal": "candidateAxisYOffset-templateAxisYOffset"
          },
          "width": {
            "signal": "barChartWidth"
          },
          "stroke": {
            "value": "white"
          }
        }
      },
      "axes": [
        {
          "orient": "bottom",
          "scale": "xscale",
          "tickCount": 3,
          "labelFlush": true
        }
      ],
      "marks": [
        {
          "type": "rect",
          "from": {
            "data": "aggregate-metric"
          },
          "encode": {
            "enter": {
              "cursor": {
                "value": "pointer"
              },
              "yc": {
                "scale": "metricScale",
                "field": "category"
              },
              "height": {
                "scale": "metricScale",
                "band": 1
              },
              "x": {
                "scale": "xscale",
                "field": "amount"
              },
              "x2": {
                "scale": "xscale",
                "value": 0
              }
            },
            "update": {
              "fill": {
                "scale": "similarityScore",
                "field": "amount"
              }
            },
            "hover": {
              "fill": {
                "value": "darkgray"
              }
            }
          }
        },
        {
          "type": "text",
          "encode": {
            "enter": {
              "align": {
                "value": "left"
              },
              "baseline": {
                "value": "bottom"
              },
              "fill": {
                "value": "black"
              }
            },
            "update": {
              "yc": {
                "scale": "metricScale",
                "signal": "tooltip.category",
                "band": 0.5
              },
              "x": {
                "scale": "xscale",
                "signal": "tooltip.amount",
                "offset": {
                  "value": 2
                }
              },
              "text": {
                "signal": "isDefined(tooltip.amount) ? format(tooltip.amount, ',.2f'): ''"
              },
              "fillOpacity": [
                {
                  "test": "datum === tooltip",
                  "value": 0
                },
                {
                  "value": 1
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "timeline",
      "type": "group",
      "data": [
        {
          "name": "communication-by-time",
          "source": [
            "template-edges",
            "candidate-edges"
          ],
          "transform": [
            {
              "type": "formula",
              "as": "date",
              "expr": "toDate(parseInt(datum.Time)*1000)"
            },
            {
              "type": "timeunit",
              "field": "date",
              "units": [
                "year",
                "month"
              ],
              "signal": "tbin"
            },
            {
              "type": "extent",
              "field": "date",
              "signal": "timeExtent"
            },
            {
              "type": "aggregate",
              "groupby": [
                "unit1",
                "Status"
              ],
              "ops": [
                "count"
              ],
              "fields": [
                null
              ],
              "as": [
                "count"
              ]
            },
            {
              "type": "stack",
              "groupby": [
                "unit1"
              ],
              "sort": {
                "field": "Status"
              },
              "field": "count"
            }
          ]
        }
      ],
